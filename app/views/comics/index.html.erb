<h1>Listing comics</h1>

<table class="table table-striped">
  <tr>
    <th>Title</th>
    <th>Description</th>
    <th></th>
  </tr>

<% @comics.each do |comic| %>
  <tr>
    <td><%= comic.title %></td>
    <td><%= comic.description %></td>
    <td><%= link_to 'Show', comic, :class=>"btn btn-info" %></td>
  </tr>
<% end %>
</table>

<br />
